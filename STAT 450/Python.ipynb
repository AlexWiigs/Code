   "source": [
    "from sympy import symbols, integrate\n",
    "\n",
    "# Define symbols\n",
    "x, t = symbols('x t')\n",
    "\n",
    "# Define the PDF of X\n",
    "pdf_x = 4*t**3\n",
    "\n",
    "# Calculate the CDF of X by integrating the PDF from 0 to x\n",
    "cdf_x = integrate(pdf_x, (t, 0, x))\n",
    "\n",
    "# Display the CDF of X\n",
    "cdf_x\n"
   ]
  }
